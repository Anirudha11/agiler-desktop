
    {{#paper-card class="a" as |card|}}
      {{#card.title as |title|}}
        {{#title.text as |text|}}
          {{#text.headline}}New{{/text.headline}}
         {{paper-divider}}
        {{#paper-list}}
        {{#each newTasks as |tasks|}}
           {{#draggable-item    remove=(action 'removeTask') content=tasks}}
              {{!-- {{#draggable-item dragged="removeTask"}} --}}
            {{#paper-item}}{{tasks}}{{/paper-item}}
            {{!-- {{/draggable-item}} --}}
            {{/draggable-item}}
            {{/each}}

        {{/paper-list}}
        {{/title.text}}
      {{/card.title}}
    {{/paper-card}}
